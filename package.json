{
  "name": "@apidog/multibot-sdk-ts",
  "version": "0.0.4",
  "description": "Telegram and VK bot SDK for TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist",
    "/README.md"
  ],
  "scripts": {
    "dev": "npm run build && node dist/main",
    "prepare": "npm run build",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint --cache --ext .ts,.tsx src"
  },
  "keywords": [
    "telegram",
    "bot",
    "vk"
  ],
  "author": "Vladislav Veluga",
  "license": "GPL-3.0",
  "dependencies": {
    "axios": "^0.19.2"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.2",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "form-data": "^3.0.0",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "bugs": {
    "url": "https://github.com/APIdogRU/multibot-sdk-ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "github:APIdogRU/multibot-sdk-ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-optional-chaining",
      "@babel/plugin-syntax-typescript",
      "@babel/transform-runtime"
    ]
  }
}
